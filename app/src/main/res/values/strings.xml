    <!-- Welcome screen / main sections -->
    <string name="welcome">Welcome to the <b>Xposed Installer</b>!\nPlease choose what you want to do:</string>
    <string name="tabInstall">Framework</string>
    <string name="tabInstallDescription">Here you can manage the Xposed Framework, which is required for any modules to work.</string>
    <string name="tabModules">Modules</string>
    <string name="tabModulesDescription">The Framework itself doesn\'t add any functionality. You need modules for that. In this section you can activate modules after you have installed them.</string>
    <string name="tabDownload">Download</string>
    <string name="tabDownloadDescription">This section gives you access to a repository of modules, so you can search and download the ones you like and keep them up to date.</string>
    <string name="welcome_framework_update_available">An update for the Xposed Framework is available (%s).</string>
    <string name="welcome_update_available">One or more update(s) for modules available.</string>
    <string name="tabSettings">Settings</string>
    <string name="tabSettingsDescription">Go to this section to configure the Xposed Installer and Framework.</string>
    <string name="tabSupport">Support</string>
    <string name="tabSupportDescription">Find help for Xposed-related issues. You can also donate to say thanks for Xposed.</string>
    <string name="tabLogs">Logs</string>
    <string name="tabLogsDescription">Display and save/send logs of the Xposed Framework.</string>
    <string name="tabAbout">About</string>
    <string name="tabAboutDescription">Learn about the people who created Xposed, where to get support for this app etc.</string>

